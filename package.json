{
  "name": "resources",
  "version": "1.0.0",
  "description": "Back-end resources",
  "scripts": {
    "watch": "babel --watch=src/main/resources --out-dir=build/resources/main --ignore \"**/*.js\"",
    "build": "concurrently \"npm-run-all build:*\"",
    "build:js": "babel src/main/resources --out-dir=build/resources/main --ignore \"**/*.js\"",
    "build:libs": "xptool build private-libs",
    "dev": "concurrently \"npm-run-all watch\""
  },
  "author": "Bouvet",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@types/jest": "^22.2.0",
    "@types/node": "^9.6.52",
    "babel-eslint": "^10.1.0",
    "concurrently": "^4.1.2",
    "deepmerge": "^4.2.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.20.0",
    "fibers": "^4.0.2",
    "npm-run-all": "^4.1.5",
    "replace": "^0.3.0",
    "watch": "^1.0.2",
    "xptool": "git+https://git.bouvet.no/scm/exm/xp-tool.git#27d5a1a"
  },
  "dependencies": {
    "mark.js": "^8.11.1",
    "lib-util-ex": "git+https://git.bouvet.no/scm/exm/lib-util-ex.git#0.7.1"
  }
}
